% prover9 < q5_d.in 

op(400,infix,oplus).
op(400,infix,otimes).
op(400,infix,heytingarrow). % heyting arrow operator
op(600,prefix,booleanalgebranot).   % Boolean Algebra unary not operator

formulas(assumptions). 
    % axims for a partial order
    x <= x.
    x <= y & y <= x -> x = y.
    x <= y & y <= z -> x <= z.

    % axioms for lattice
    x <= Top.
    Bot <= x.
    z <= x otimes y <-> z <= x & z <= y.
    x oplus y <= z <-> x <= z & y <= z.

    % distributivity
    (x oplus y) otimes z = (x otimes z) oplus (y otimes z).
    (x otimes y) oplus z = (x oplus z) otimes (y oplus z).


    % Boolean Algebra
    (x oplus (booleanalgebranot x)) = Top.
    (x otimes (booleanalgebranot x)) = Bot.

    % (D)
    (x heytingarrow y) = ((booleanalgebranot x) oplus y).

end_of_list.

formulas(goals).



    % Heyting Algebra
    exists x exists y exists z ((x otimes y) <= z) <-> (x <= (y heytingarrow z)).
end_of_list.
